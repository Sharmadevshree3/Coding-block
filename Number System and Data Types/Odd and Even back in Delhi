#include <iostream>
#include <string>
using namespace std;

int main() {
    int t;
    cin >> t; 

    while (t--) {
        string n;
        cin >> n; 

        int sum = 0;   
        int total = 0; 

        for (int i = 0; i < n.length(); i++) {
            int number = n[i] - '0';
            if (number % 2 == 0) {
                sum += number;
            } else {
                total += number;
            }
        }

        if (sum % 4 == 0 || total % 3 == 0) {
            cout << "Yes" << endl;
        } else {
            cout << "No" << endl;
        }
    }

    return 0;
}
